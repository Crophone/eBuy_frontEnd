.zan-font-bold {
  font-weight: 700;
}
.zan-font-18 {
  font-size: 16px;
}
.orders {
  display: -webkit-box;
  display: flex;
  margin-left: 0;
  padding: 5px 15px;
  overflow: hidden;
  position: relative;
  font-size: 14px;
}
.orders__detail {
  -webkit-box-flex: 1;
  flex: 1;
  margin-left: 10px;
  position: relative;
}

.orders__detail-row {
  overflow: hidden;
  line-height: 20px;
  min-height: 20px;
  margin-bottom: 3px;
  word-break:break-all;
}

.orders__right-col {
  float: right;
}

.orders__left-col {
  margin-right: 80px;
}
